{
    "name": "bouncefirst/plaid",
    "type": "library",
    "description": "Laravel specific wrapper for Plaid. Bankdata for developers.",
    "keywords": ["Plaid", "Laravel"],
    "homepage": "https://bouncefirst.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Ben Warburton",
            "email": "ben@travoltron.com"
        },
        {
            "name": "Lance Pioch",
            "email": "me@lance.sh"
        }
    ],
    "require": {
        "illuminate/support": "~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "php": "^7.1.3",
        "guzzlehttp/guzzle": "~6.0",
        "nesbot/carbon": "~1.18"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*"
    },
    "autoload": {
        "psr-4": {
            "Bouncefirst\\Plaid\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bouncefirst\\Plaid\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
